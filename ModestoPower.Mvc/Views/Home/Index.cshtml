<section id="customer" class="parallax-customer parallax" style="height: 500px; ">
    <div class="container" style="margin-top: 100px;">
        
        <h1>modesto power blog</h1>
    </div>
</section>

<section id="blog">
    <div class="container">
        <h2>Latest Posts</h2>
        <h3>Keep up to date with the wonderful world of Modesto Power</h3>
        <h4></h4>
    </div>
    <div class="projects-wrapper">
        <div class="projects">
            @foreach (var b in Model.Posts.Take(5))
            {
                <div class="project ajax-project type3">
                <div class="visual">
                    <img src="@b.imagepath" alt="@b.title">
                    <div class="hover">
                        <ul>
                            <li>
                                <a href="#">
                                    <span class="icon-facebook"></span>
                                    <span>share</span>
                                </a>
                            </li>
                            <li>
                                <a href="#">
                                    <span class=" icon-twitter"></span>
                                    <span>tweet</span>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
                <div class="project-content" style="height: 75px;">
                    <h4><a href="/blog/@b.category.Replace(" ", "-")/@b.title.Replace(" ", "-")" class="pop-project">@b.title</a></h4>
                    <a class="visit" href="/blog/@b.category.Replace(" ", "-")/@b.title.Replace(" ", "-")"></a>
                </div>
            </div><!--project-->
            }
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="clearfix"></div>
</section>

