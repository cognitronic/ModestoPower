<section id="contact">
    <div class="container">
        <h2>get in touch</h2>
        <h3>Don't be shy, give us a shout.</h3>
        <div class="row">
            <form method="POST" action="Contact/SendMessage" id="contact-form" class="default-form validate">
                <div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <div class="contactus">
                        <h5>Sign Up For Coupons & Discounts</h5>
                        <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                            <input type="text" class="form-control required" name="field_name" id="name" placeholder="Name"><br />
                            <input type="email" class="form-control required email" name="field_email" id="email" placeholder="Email"><br />
                            <input type="tel" class="form-control required tel" name="field_phone" id="phone" placeholder="Phone"><br />
                            <input type="text" class="form-control required" name="field_subject" id="subject" placeholder="Subject"><br />
                            <textarea name="field_message" class="form-control required" id="message" placeholder="Message"></textarea><br />
                            <button id="submit" class="btn btn-primary">submit</button>
                        </div>
                        <div id="valid-issue" style="display:none;"> Please Provide Valid Information</div>
                    </div>
                </div>
                <div class="interested col-lg-6 col-md-6 col-sm-6 col-xs-12">
                    <h5>What are you interested in?</h5>
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <input type="checkbox" name="interestedbjj" id="interestedbjj">Brazilian Jiu Jitsu<br />
                        <input type="checkbox" name="interestedmuaythai" id="interestedmuaythai">Muay Thai<br />
                        <input type="checkbox" name="interestedmma" id="interestedmma">MMA<br />
                        <input type="checkbox" name="interestedkickboxing" id="interestedkickboxing">Cardio Kickboxing<br />
                    </div>
                    <div class="col-lg-6 col-md-6 col-sm-6 col-xs-6">
                        <input type="checkbox" name="interestedtpl" id="interestedtpl">The Performance Lab<br />
                        <input type="checkbox" name="interestedpeak" id="interestedpeak">Peak Physique<br />
                        <input type="checkbox" name="interestedweightloss" id="interestedweightloss"> Weight Loss<br />
                        <input type="checkbox" name="interestedstrength" id="interestedstrength">Increase Strength<br />
                    </div>
                </div>
            </form>
        </div>
    </div>
</section>


